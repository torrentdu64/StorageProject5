@model  IEnumerable<StorageProject5.Models.Furniture>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index Rental</h2>


<div class="container">
    <div class="row">
        @foreach (var item in Model)
        {
            <div class="panel panel-default">
                <div class="panel-body">
                    <h1>@item.Name - <span class="label label-warning">Available</span></h1>
                    <h3></h3>
                </div>
                <div class="panel-footer">
                    <div class="btn btn-primary">Rented</div>
                </div>
            </div>

        }
    </div>
</div>
